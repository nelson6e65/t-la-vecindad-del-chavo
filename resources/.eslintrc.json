{
    "extends": [
        "standard",
        "plugin:vue/recommended"
    ],
    "plugins": [
        "vue"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 8
    },
    "env": {
        "node": true,
        "browser": true
    },
    "globals": {
    },
    "rules": {
        "no-console": 1,
        "no-unused-vars": 1,
        "strict": [1, "global"],
        "vue/max-attributes-per-line": [1, {
            "singleline": 10,
            "multiline": {
                "max": 1,
                "allowFirstLine": true
            }
        }]
    }
}
